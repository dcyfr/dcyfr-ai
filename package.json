{
  "name": "@dcyfr/ai",
  "version": "1.0.4",
  "description": "Portable AI agent framework with plugin architecture",
  "main": "./dist/ai/index.js",
  "types": "./dist/ai/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/ai/index.d.ts",
      "import": "./dist/ai/index.js"
    },
    "./mcp": {
      "types": "./dist/ai/mcp/index.d.ts",
      "import": "./dist/ai/mcp/index.js"
    }
  },
  "bin": {
    "dcyfr-ai": "./bin/dcyfr-ai.js",
    "dcyfr-ai-tui": "./bin/tui.js",
    "dcyfr": "./dist/ai/cli/telemetry-dashboard.js"
  },
  "scripts": {
    "build": "tsc && node scripts/fix-esm-imports.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint packages/",
    "prepack": "npm run build",
    "cli": "node bin/cli.js",
    "tui": "node bin/tui.js",
    "mcp:analytics": "tsx packages/ai/mcp/servers/analytics/index.ts",
    "mcp:tokens": "tsx packages/ai/mcp/servers/design-tokens/index.ts",
    "mcp:content": "tsx packages/ai/mcp/servers/content-manager/index.ts",
    "mcp:promptintel": "tsx packages/ai/mcp/servers/promptintel/index.ts",
    "mcp:delegation": "tsx packages/ai/mcp/servers/delegation-monitor/index.ts",
    "mcp:dev:analytics": "npx fastmcp dev packages/ai/mcp/servers/analytics/index.ts",
    "mcp:dev:tokens": "npx fastmcp dev packages/ai/mcp/servers/design-tokens/index.ts",
    "mcp:dev:content": "npx fastmcp dev packages/ai/mcp/servers/content-manager/index.ts",
    "mcp:dev:promptintel": "npx fastmcp dev packages/ai/mcp/servers/promptintel/index.ts",
    "mcp:dev:delegation": "npx fastmcp dev packages/ai/mcp/servers/delegation-monitor/index.ts",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "release": "npm run build && changeset publish",
    "version:patch": "npm version patch -m 'chore: bump version to %s'",
    "version:minor": "npm version minor -m 'chore: bump version to %s'",
    "version:major": "npm version major -m 'chore: bump version to %s'"
  },
  "keywords": [
    "ai",
    "agent",
    "framework",
    "plugin",
    "llm",
    "claude",
    "copilot",
    "automation"
  ],
  "author": "DCYFR Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcyfr/dcyfr-ai.git"
  },
  "bugs": {
    "url": "https://github.com/dcyfr/dcyfr-ai/issues"
  },
  "homepage": "https://github.com/dcyfr/dcyfr-ai#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@azure/identity": "^4.0.0",
    "@azure/search-documents": "^12.0.0",
    "@google/genai": "^1.2.0",
    "@langchain/core": "^1.1.24",
    "@mistralai/mistralai": "^1.5.2",
    "@qdrant/js-client-rest": "^1.16.2",
    "@supabase/supabase-js": "^2.49.1",
    "openai": "^4.104.0",
    "@upstash/redis": "^1.34.3",
    "axios": "^1.8.2",
    "better-sqlite3": "^11.8.1",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "cloudflare": "^4.2.0",
    "commander": "^12.0.0",
    "fastmcp": "^3.33.0",
    "groq-sdk": "0.3.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^13.2.2",
    "mem0ai": "2.2.2",
    "neo4j-driver": "^5.28.1",
    "ollama": "^0.5.14",
    "ora": "^9.1.0",
    "pg": "8.11.3",
    "redis": "^4.6.13",
    "yaml": "^2.3.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/better-sqlite3": "^7.6.12",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.39.2",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "overrides": {
    "tar": "^7.5.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "bin",
    "config",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
