{
  "name": "@dcyfr/ai",
  "version": "1.0.3",
  "description": "Portable AI agent framework with plugin architecture",
  "main": "./dist/ai/index.js",
  "types": "./dist/ai/index.d.ts",
  "type": "module",
  "bin": {
    "dcyfr-ai": "./bin/dcyfr-ai.js",
    "dcyfr-ai-tui": "./bin/tui.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint packages/",
    "prepack": "npm run build",
    "cli": "node bin/cli.js",
    "tui": "node bin/tui.js",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "release": "npm run build && changeset publish",
    "version:patch": "npm version patch -m 'chore: bump version to %s'",
    "version:minor": "npm version minor -m 'chore: bump version to %s'",
    "version:major": "npm version major -m 'chore: bump version to %s'"
  },
  "keywords": [
    "ai",
    "agent",
    "framework",
    "plugin",
    "llm",
    "claude",
    "copilot",
    "automation"
  ],
  "author": "DCYFR Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcyfr/dcyfr-ai.git"
  },
  "bugs": {
    "url": "https://github.com/dcyfr/dcyfr-ai/issues"
  },
  "homepage": "https://github.com/dcyfr/dcyfr-ai#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "inquirer": "^13.2.2",
    "ora": "^9.1.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "bin",
    "config",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
